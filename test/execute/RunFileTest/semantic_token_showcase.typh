import re
from pathlib import Path

class C {
    let x: int;
    def __init__() {
        self.x = 10
    }
}

def fun(file: Path) {
    #(Note this is executed in .typhon directory)#
    with (let f = open(str(file), "r")) {
        let content = f.read()
        let m = re.match(r'^import (?P<name>\w+)\n', content)
        print(f"match: {m?.group?("name")}")
    }
}
var my_dir: Path = Path(__file__).parent.parent # RunFileTest
if (my_dir.exists()) {
    let typh_path = my_dir / "semantic_token_showcase.typh"
    (try fun(typh_path) except print("File not found"))
}

